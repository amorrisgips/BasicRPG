<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_overworld_screen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/backgroundforest"
    tools:context="com.example.sierra.basicrpg.OverworldScreen">

    <ImageView
        android:layout_width="60dp"
        android:layout_height="73dp"
        app:srcCompat="@drawable/nullimage"
        android:layout_marginTop="22dp"
        android:id="@+id/cell1"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/uparrow"
        android:layout_alignStart="@+id/uparrow" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="73dp"
        app:srcCompat="@drawable/nullimage"
        android:layout_marginRight="45dp"
        android:layout_marginEnd="45dp"
        android:id="@+id/cell3"
        android:layout_alignTop="@+id/cell2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="73dp"
        app:srcCompat="@drawable/nullimage"
        android:id="@+id/cell2"
        android:layout_alignTop="@+id/cell1"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="73dp"
        app:srcCompat="@drawable/nullimage"
        android:id="@+id/cell7"
        android:layout_alignTop="@+id/cell8"
        android:layout_alignLeft="@+id/cell4"
        android:layout_alignStart="@+id/cell4" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="73dp"
        app:srcCompat="@drawable/nullimage"
        android:id="@+id/cell5"
        android:layout_alignTop="@+id/cell4"
        android:layout_alignLeft="@+id/cell2"
        android:layout_alignStart="@+id/cell2" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="73dp"
        app:srcCompat="@drawable/nullimage"
        android:id="@+id/cell6"
        android:layout_alignTop="@+id/cell5"
        android:layout_alignLeft="@+id/cell3"
        android:layout_alignStart="@+id/cell3" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="73dp"
        app:srcCompat="@drawable/nullimage"
        android:layout_marginTop="30dp"
        android:id="@+id/cell4"
        android:layout_below="@+id/cell1"
        android:layout_alignLeft="@+id/cell1"
        android:layout_alignStart="@+id/cell1" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="73dp"
        app:srcCompat="@drawable/nullimage"
        android:id="@+id/cell8"
        android:layout_alignTop="@+id/cell9"
        android:layout_alignLeft="@+id/cell5"
        android:layout_alignStart="@+id/cell5" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="73dp"
        app:srcCompat="@drawable/nullimage"
        android:id="@+id/cell9"
        android:layout_marginTop="50dp"
        android:layout_below="@+id/cell6"
        android:layout_alignLeft="@+id/cell6"
        android:layout_alignStart="@+id/cell6" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/buttonf"
        android:id="@+id/selectButton"
        android:background="@null"
        android:layout_alignTop="@+id/uparrow"
        android:layout_alignRight="@+id/cell7"
        android:layout_alignEnd="@+id/cell7"
        android:layout_marginTop="37dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/arrowleft"
        android:background="@null"
        android:id="@+id/leftarrow"
        android:onClick="moveLeft"
        android:layout_below="@+id/cell7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="52dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/arrowup"
        android:background="@null"
        android:id="@+id/uparrow"
        android:onClick="moveUp"
        android:layout_below="@+id/cell7"
        android:layout_toRightOf="@+id/leftarrow"
        android:layout_toEndOf="@+id/leftarrow" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/arrowright"
        android:background="@null"
        android:onClick="moveRight"
        android:id="@+id/rightarrow"
        android:layout_alignTop="@+id/leftarrow"
        android:layout_toRightOf="@+id/downarrow"
        android:layout_toEndOf="@+id/downarrow" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/arrowdown"
        android:background="@null"
        android:id="@+id/downarrow"
        android:onClick="moveDown"
        android:layout_below="@+id/leftarrow"
        android:layout_toRightOf="@+id/leftarrow"
        android:layout_toEndOf="@+id/leftarrow" />

    <TextView
        android:text="GO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:layout_alignTop="@+id/leftarrow"
        android:layout_toRightOf="@+id/leftarrow"
        android:layout_toEndOf="@+id/leftarrow"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="16dp" />

</RelativeLayout>
